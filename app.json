{
  "expo": {
    "name": "Employee-List",
    "slug": "employees-list",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./shared/assets/images/icon.png",
    "scheme": "employeelist",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "CFBundleLocalizations": ["ru", "en"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#8a66ff",
        "foregroundImage": "./shared/assets/images/main-icon.png",
        "backgroundImage": "./shared/assets/images/main-background.png",
        "monochromeImage": "./shared/assets/images/main-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.hirehoffee.employeeslist"
    },
    "web": {
      "output": "static",
      "favicon": "./shared/assets/images/main-icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./shared/assets/images/main-splash.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#181818",
            "image": "./shared/assets/images/main-splash-dark.png"
          }
        }
      ],
      [
        "expo-localization",
        {
          "supportedLocales": {
            "ios": ["ru", "en"],
            "android": ["ru", "en"]
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "021d49e6-1947-4f4a-ae02-8330b12a5a3c"
      }
    },
    "owner": "hirehoffee"
  }
}
